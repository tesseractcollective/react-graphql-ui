{
  "name": "@tesseractcollective/react-graphql-ui",
  "version": "0.4.2beta2",
  "description": "A library built on top of react-graphql to give you various components and hooks to help speed up development. Currently include features for DataTables, Forms, and Dropdowns based on DB relationships",
  "keywords": [
    "graphql",
    "react-graphql",
    "hasura",
    "datatable"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm webmode && vite build && ttsc --project tsconfig-web.build.json",
    "tsc": "ttsc --project tsconfig-web.build.json",
    "pack": "pnpm pack",
    "pub": "pnpm publish --access public",
    "webmode": "node ./scripts/enterwebmode.js",
    "devmode": "node ./scripts/enterdevmode.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tesseractcollective/react-graphql-ui.git"
  },
  "author": "@tesseractcollective",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tesseractcollective/react-graphql-ui/issues"
  },
  "homepage": "https://github.com/tesseractcollective/react-graphql-ui#readme",
  "devDependencies": {
    "@tesseractcollective/react-graphql": "0.9.17",
    "@types/history": "4.7.9",
    "@types/lodash": "4.14.177",
    "@types/react": "17.0.37",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "case": "^1.6.3",
    "graphql": "15.7.2",
    "jotai": "1.4.6",
    "lodash": "4.17.21",
    "query-string": "7.0.1",
    "quill": "^1.3.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.20.5",
    "react-transition-group": "4.4.2",
    "ttypescript": "^1.5.12",
    "typescript": "4.5.2",
    "typescript-transform-paths": "3.3.1",
    "urql": "2.0.5",
    "vite": "2.6.14",
    "vite-tsconfig-paths": "3.3.17"
  },
  "peerDependencies": {
    "@tesseractcollective/react-graphql": "0.9.17",
    "jotai": "1.4.6",
    "quill": "^1.3.7",
    "react-hook-form": "7.20.5",
    "react-transition-group": "4.4.2"
  },
  "main": "dist/react-graphql-ui.es.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/react-graphql-ui.es.js"
    }
  },
  "dependencies": {
    "primereact": "7.2.1"
  }
}